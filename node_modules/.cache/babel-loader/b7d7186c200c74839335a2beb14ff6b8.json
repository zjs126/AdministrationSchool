 bit) {\n          group.push((1 << bit).toString());\n        }\n      }\n\n      this.priGroup = group;\n    },\n    readPrivilegeFromGroup() {\n      let privilege = 0;\n      for (let i = 0; i < this.priGroup.length; i++) {\n        privilege += parseInt(this.priGroup[i]);\n      }\n      return privilege;\n    },\n    save() {\n      this.entityForm.privilege = this.readPrivilegeFromGroup();\n      if (this.entityForm.id === -1) {\n        api.create(this.entityForm).then(() => {\n          this.finishSave();\n        });\n      } else {\n        api.update(this.entityForm).then(() => {\n          this.finishSave();\n        });\n      }\n    },\n    finishSave() {\n      this.$message.success(\"成功\");\n      this.getList();\n      this.editing = false;\n    },\n    deleteItem(id) {\n      api.deleteItem(id).then(() => {\n        this.$message.success(\"删除成功\");\n        this.getList();\n      });\n    }\n  },\n  created() {\n    this.getList();\n  }\n};\n</script>\n\n<style scoped></style>\n"],"mappings":";;;AAuEA,YAAAA,GAAA;AACA,OAAAC,UAAA;AAEA;EACAC,IAAA;EACAC,IAAA,WAAAA,KAAA;IACA;MACAC,UAAA;MACAC,QAAA;MACAC,SAAA;MACAC,OAAA;IACA;EACA;EACAC,OAAA;IACAC,OAAA,WAAAA,QAAA;MAAA,IAAAC,KAAA;MACAV,GAAA,CAAAW,IAAA,GAAAC,IAAA,WAAAC,GAAA;QACAA,GAAA,GAAAH,KAAA,CAAAI,iBAAA,CAAAD,GAAA;QACAH,KAAA,CAAAJ,SAAA,GAAAO,GAAA;MACA;IACA;IACAC,iBAAA,WAAAA,kBAAAX,IAAA;MACA,SAAAY,CAAA,MAAAA,CAAA,GAAAZ,IAAA,CAAAa,MAAA,EAAAD,CAAA;QACA,IAAAE,KAAA;QACA,SAAAC,GAAA,MAAAA,GAAA,GAAAjB,UAAA,CAAAkB,eAAA,EAAAD,GAAA;UACA,UAAAA,GAAA,GAAAf,IAAA,CAAAY,CAAA,EAAAK,SAAA,WAAAF,GAAA;YACAD,KAAA;UACA;QACA;QACAd,IAAA,CAAAY,CAAA,EAAAM,UAAA,GAAAJ,KAAA;MACA;MAEA,OAAAd,IAAA;IACA;IACAmB,MAAA,WAAAA,OAAA;MACA,KAAAjB,QAAA;MACA,KAAAD,UAAA;QACAmB,EAAA;QACAC,QAAA;QACAJ,SAAA;QACAK,QAAA;MACA;MAEA,KAAAlB,OAAA;IACA;IACAmB,IAAA,WAAAA,KAAAH,EAAA;MAAA,IAAAI,MAAA;MACA3B,GAAA,CAAA4B,GAAA,CAAAL,EAAA,EAAAX,IAAA,WAAAC,GAAA;QACAc,MAAA,CAAAE,WAAA,CAAAhB,GAAA;QACAc,MAAA,CAAAvB,UAAA,GAAAS,GAAA;QACAc,MAAA,CAAApB,OAAA;MACA;IACA;IACAsB,WA