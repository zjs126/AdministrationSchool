    });\n    },\n    save() {\n      let day = this.courseDay + 1;\n      let time = this.courseTime + 1;\n      this.entityForm.time = day + \"-\" + time + \"-\" + this.courseLength;\n\n      if (this.entityForm.id === -1) {\n        api.create(this.entityForm).then(() => {\n          this.finishSave();\n        });\n      } else {\n        api.update(this.entityForm).then(() => {\n          this.finishSave();\n        });\n      }\n    },\n    finishSave() {\n      this.$message.success(\"成功\");\n      this.getPage(this.pageIndex);\n      this.editing = false;\n    },\n    deleteItem(id) {\n      api.deleteItem(id).then(() => {\n        this.$message.success(\"删除成功\");\n        this.getPage(this.pageIndex);\n      });\n    },\n    getTeachers() {\n      teacherApi.listName().then(res => {\n        this.teachers = res;\n      });\n    }\n  },\n  created() {\n    this.query();\n    this.getTeachers();\n  }\n};\n</script>\n\n<style scoped></style>\n"],"mappings":";AAkKA,YAAAA,GAAA;AACA,YAAAC,UAAA;AAEA;EACAC,IAAA;EACAC,IAAA,WAAAA,KAAA;IACA;MACAC,SAAA;QACAC,cAAA;QACAC,WAAA;QACAJ,IAAA;MACA;MACAK,UAAA;MACAC,SAAA;MACAC,QAAA,EAAAT,GAAA,CAAAS,QAAA;MACAC,SAAA;MACAC,SAAA;MACAC,OAAA;MACAC,QAAA;MACAC,SAAA;MACAC,UAAA;MACAC,YAAA;MACAC,IAAA,GACA,OACA,OACA,OACA,OACA,OACA,OACA,MACA;MACAC,KAAA,GACA,OACA,OACA,OACA,OACA,OACA,OACA,OACA,OACA;IAEA;EACA;EACAC,OAAA;IACAC,KAAA,WAAAA,MAAA;MAAA,IAAAC,KAAA;MACArB,GAAA,CACAsB,YAAA,CACA,KAAAlB,SAAA,CAAAC,cAAA,EACA,KAAAD,SAAA,CAAAE,WAAA,EACA,KAAAF,SAAA,CAAAF,IACA,EACAqB,IAAA,WAAAC,GAAA;QACAH,KAAA,CAAAX,SAAA,GAAAc,GAAA;QACAH,KAAA,CAAAV,SAAA;QACAU,KAAA,CAAAI,OAAA;MACA;IACA;IACAA,OAAA,WAAAA,QAAAd,SAAA;MAAA,IAAAe,MAAA;MACA1B,GAAA,CACAyB,OAAA,CACAd,SAAA,EACA,KAAAP,SAAA,CAAAC,cAAA,EACA,KAAAD,SAAA,CAAAE,WAAA,EACA,KAAAF,SAAA,CAAAF,IACA,EACAqB,IAAA,WAAAC,GAAA;QACAE,MAAA,CAAAlB,SAAA,GAAAgB,GAAA;MACA;IACA;IACAG,MAAA,WAAAA,OAAA;MACA,KAAApB,UAAA;QACAqB,EAAA;QACAC,SAAA;QACA3B,IAAA;QACA4B,KAAA;QACAC,IAAA;QACAC,QAAA;QACAC,MAAA;QACAC,OAAA;QACAC,QAAA;QACAC,YAAA;MAC