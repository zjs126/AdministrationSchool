update(this.entityForm).then(() => {\n        this.$message.success(\"更新信息成功!\");\n      });\n    }\n  },\n  created() {\n    this.get();\n  }\n};\n</script>\n\n<style scoped>\n.info-form {\n  min-width: 400px;\n  width: 35%;\n  margin: auto;\n}\n</style>\n"]}]}